<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="triangle" class="org.cahyana.demo.Triangle">
		<property name="pointA" ref="point1"></property>
		
		<property name="pointB">
			<bean id="point2" class="org.cahyana.demo.Point">
				<property name="x" value="-20"></property>
				<property name="y" value="0"></property>
			</bean>
		</property>
		
		<property name="pointC">
			<bean id="point3" class="org.cahyana.demo.Point">
				<property name="x" value="20"></property>
				<property name="y" value="0"></property>
			</bean>
		</property>
	</bean>

	<bean id="point1" class="org.cahyana.demo.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	
	<alias name="triangle" alias="triangle-alias"/>
</beans>